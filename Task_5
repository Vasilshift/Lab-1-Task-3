
//Даны действительные числа x, y, z. Вывести их на экран наименьшее из них.

public class digits {
	
	public static void main(String args[]){
	      int x = 8;
	      int y = 5;
	      int z = 8;    
	      
	      if (x > z & y > z) {
	    	  	    	      	  
	    	  System.out.println(z);
	    	  
	      }
	      
	      if (x < y & x < z) {
	    	  
	    	  System.out.println(x);
	    	  
	      }
	      
	      if (y < x & y < z) {
	    	  
	    	  System.out.println(y);
	    	  
	      }	      
	    
   
	}   
	

}
